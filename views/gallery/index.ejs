<% include ../partials/header %>


    <div class="container">
    <div class="row">
    <% gallery.forEach(function(gallery){ %>
        <div class="col-lg-4 col-sm-6">
            <div class="thumbnail photos">
                <img src="<%= gallery.cover%>">
                <h3><a href="gallery/<%=gallery._id%>"><%= gallery.title %></a></h3>
            </div>
        </div>
    <% }); %>

